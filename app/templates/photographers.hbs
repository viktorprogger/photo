<h1>List of photographers</h1>

    <ul>
        {{#each this.model as |photographer|}}
            <li>
                <img src={{ photographer.avatar }} alt={{ photographer.name }}>
                <a href="#">{{ photographer.name }}</a>
                <br>
                Начал в {{ photographer.yearStart }} г.,
                {{ photographer.experience }} лет опыта
            </li>
        {{/each}}
    </ul>